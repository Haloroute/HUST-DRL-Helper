{
  "manifest_version": 3,
  "name": "HUST DRL Helper",
  "description": "Hỗ trợ lấy điểm rèn luyện trên Microsoft Form cho sinh viên HUST! Hình ảnh: Flaticon.com",
  "version": "0.2",
  "permissions": [
    "storage",
    "scripting",
    "activeTab",
    "tabs"
  ],
  "action": {
    "default_popup": "scripts/popup.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
