{
  "manifest_version": 3,
  "name": "HUST DRL Helper",
  "description": "Hỗ trợ lấy điểm rèn luyện trên Microsoft Form cho sinh viên HUST!",
  "version": "0.8.3",
  "permissions": [
    "scripting",
    "activeTab"
  ],
  "action": {
    "default_popup": "scripts/main.html"
  },
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  }
}
